{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "dist/2ndmarket",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/privacy.html",
        "destination": "privacy.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
